{
  "cli": {
    "version": ">= 3.9.1",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "env": {
        "APP_NAME": "Poolp"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "autoIncrement": true,
      "channel": "production"
    },
    "development": {
      "env": {
        "APP_NAME": "Poolp (Dev)"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "development"
    },
    "development-simulator": {
      "env": {
        "APP_NAME": "Poolp (Dev)"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    },
    "preview": {
      "env": {
        "APP_NAME": "Poolp (Preview)"
      },
      "channel": "preview",
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6621246983"
      }
    }
  }
}
